<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>ServletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>formCadBook</servlet-name>
        <servlet-class>formCadBook</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cadPage</servlet-name>
        <servlet-class>cadPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cadCategoria</servlet-name>
        <servlet-class>cadCategoria</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SQLSelectTag</servlet-name>
        <servlet-class>SQLSelectTag</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GenericSQLSelectTag</servlet-name>
        <servlet-class>GenericSQLSelectTag</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/ServletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>formCadBook</servlet-name>
        <url-pattern>/formCadBook</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cadPage</servlet-name>
        <url-pattern>/cadPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cadCategoria</servlet-name>
        <url-pattern>/cadCategoria</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SQLSelectTag</servlet-name>
        <url-pattern>/SQLSelectTag</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenericSQLSelectTag</servlet-name>
        <url-pattern>/GenericSQLSelectTag</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
